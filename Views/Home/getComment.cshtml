<h1 style=" margin-top:80px;font-family:Arial Rounded MT Bold;font:bold;">All Messages</h1>

@foreach (var j in @Model)
{

    <div class="row text-center">
        <div class="col-lg-3">
        </div>
        <div class=" col-lg-5">
            <br />
            <fieldset style="font-size:14px;border-radius:8px">
                <div style="margin:30px">
                    <p> <b>Contact Type : </b>@j.contactType</p>
                    <p> <b>Email Address : </b> @j.Email</p>

                    <p><b>Name :</b> @j.Name</p>

                    <p><b>Message :</b> @j.</p>
                </div>
            </fieldset>
        </div>
    </div>
}